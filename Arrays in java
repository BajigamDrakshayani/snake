class AdditionMultiplication
{
public static void main(String a [])
{ int a[][],b[][],c[][];
a[][]=new int[2][2];
b[][]=new int[2][2];
c[][]=new int[2][2];
a[][]={{1,2,3},{2,3,4},{3,4,5}};
b[][]={{9,8,7},{8,7,6},{7,6,5}};
for(int i=0;i<2;i++)
{
for(int j=0;j<2;j++)
{
System.out.println("c["+i+"]["+j+"]="+a[i][j]+b[i][j]);
c[i][j]=a[i][j]*b[i][j];
System.out.println(c[i][j]);
}
}
}
}
